<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dev.sbytmacke.onlyprofit.controllers.UserDetailController">
	<VBox alignment="CENTER" prefWidth="350.0" spacing="20.0">
		<Label fx:id="usernameLabel" text="Username" underline="true">
			<padding>
				<Insets left="10.0" right="10.0" top="10.0" />
			</padding>
			<font>
				<Font size="16.0" />
			</font>
		</Label>
		<GridPane fx:id="gridPane" alignment="CENTER" prefHeight="92.0">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" maxWidth="195.0" minWidth="10.0" prefWidth="193.0" />
				<ColumnConstraints hgrow="SOMETIMES" />
			</columnConstraints>
			<VBox.margin>
				<Insets left="10.0" right="10.0" />
			</VBox.margin>
		</GridPane>
		<Button onAction="#closeWindow" text="Close">
			<font>
				<Font size="13.0" />
			</font>
			<VBox.margin>
				<Insets bottom="20.0" />
			</VBox.margin>
		</Button>
	</VBox>
</AnchorPane>
